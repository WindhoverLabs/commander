.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 General
    div.widget-body.table-responsive
      table.table.table-condensed
            tbody
              tr
                th Cmd Count
                td
                  div.sage-tlm(data-commander={tlm:[{name:'/CFE/ES_HK/Payload.CFEMinorVersion'}]} data-format='text') ---
              tr
                th Error Count
                td
                  div(data-commander={tlm:[{name:'/CFE/ES_HK/Payload.ProcessorResets'}]} data-format='text') ---   
    footer.btn-group(role="group")
      button.btn.btn-primary(data-sage={cmd:{name:'/CFS/FLOW/NoOp'}}) No-Op
      button.btn.btn-primary(data-sage={cmd:{name:'/CFS/FLOW/Reset'}}) Reset      
